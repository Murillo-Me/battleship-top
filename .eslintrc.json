{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "no-shadow": "off",
        "linebreak-style": "off",
        "no-param-reassign": [1, { "props": false }],
        "max-len": "warn"
    }
}
